      "@babel/preset-env",
      {
        "loose": true,
        "useBuiltIns": "entry",
        "shippedProposals": true,
        "modules": "commonjs",
        "targets": {
          "node": 6
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ]
}
